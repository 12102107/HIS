package com.znyy.bean;

 /** 
 * @类名称: SmDrugOrGoods
 * @类描述:使用贵重或自费药品(物品)定义表
 * @作者:付士山
 * @创建时间:2016-1-8 上午9:03:43
 */
 
public class SmDrugOrGoods implements java.io.Serializable {

	private static final long serialVersionUID = 1L;
	private Integer code;
	private String name;
	private String price;
	private String enable;
	private String type;

	// Constructors

	/** default constructor */
	public SmDrugOrGoods() {
	}

	/** full constructor */
	public SmDrugOrGoods(String name, String price, String enable, String type) {
		this.name = name;
		this.price = price;
		this.enable = enable;
		this.type = type;
	}

	public String getName() {
		return this.name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPrice() {
		return this.price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getEnable() {
		return this.enable;
	}

	public void setEnable(String enable) {
		this.enable = enable;
	}

	public String getType() {
		return this.type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Integer getCode() {
	
		return code;
	}

	public void setCode(Integer code) {
	
		this.code = code;
	}

}