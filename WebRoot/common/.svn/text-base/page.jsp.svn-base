<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<div id="paging_div" style="float: right;margin-right: 27px;">
	<span style="float: left; margin-top: 25px; margin-right: 15px;">&nbsp;&nbsp;共<font
		color="#178EB9">&nbsp;${totalN}&nbsp;</font>条记录 当前第<font
		color="#178EB9">&nbsp;${page}/${totalP}&nbsp;</font>页 跳转到 第&nbsp;<input
		id="gopage" name="gopage" style="width:25px" type="text"
		onkeyup="value=value.replace(/[^\d]/g,'') "
		onbeforepaste="clipboardData.setData('text',clipboardData.getData('text').replace(/[^\d]/g,''))" />
		&nbsp;页&nbsp;<input type="button" class="but white" value="Go" onclick="gotopage()" />
	</span>
	<ul class="pagination" id="pageurl"></ul>
</div>
<br><br><br><br><br><br>