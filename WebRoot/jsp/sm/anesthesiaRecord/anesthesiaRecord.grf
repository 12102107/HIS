											CustomDrawScript='function GetColorValue(r,g,b)\r\n{\r\n   return r + g*256 + b*256*256;\r\n}\r\n\r\nvar Graphics = Report.Graphics;\r\nvar startY = 419 ;\r\nvar endY = 803;\r\nvar startX = 101 ;\r\nvar endX = 660 ;\r\nvar interval = 9.33 ;\r\nvar toTop = 327;\r\n\r\n    //画 小列\r\nfor(var col = 1;col < 60; col++){\r\n        var x = col * 9.33;\r\n        Graphics.SelectPen(1, GetColorValue(0, 0, 0), 0/*grpsSolid*/);\r\n        Graphics.MoveTo( x + startX ,  startY ) ;\r\n        Graphics.LineTo( x + startX ,  endY ) ;\r\n        Graphics.RestorePen();\r\n    }\r\n    //画 小行\r\nfor(var row = 1;row < 24; row++){\r\n        var y = row*15.11;\r\n        Graphics.SelectPen(1, GetColorValue(0, 0, 0), 0/*grpsSolid*/);\r\n        Graphics.MoveTo( startX ,  y + startY ) ;\r\n        Graphics.LineTo( endX ,  y + startY );\r\n        Graphics.RestorePen();\r\n    }\r\n\r\n//呼吸\r\nvar resprateYs = Report.FieldByName("RESPRATEYS").AsString;\r\n     if ( resprateYs != "" ){\r\n           var resprateY = resprateYs.split(",");\r\n           for( var i = 0 ; i < resprateY.length ;  i++ ){\r\n               if ( i > 0 ){\r\n                   Graphics.SelectPen(1, GetColorValue(0, 0, 255), 0);\r\n                   Graphics.MoveTo((resprateY[i-1].split(":")[0])-(-startX),  resprateY[i-1].split(":")[1]-(-toTop ));\r\n                   Graphics.LineTo((resprateY[i].split(":")[0])-(-startX), resprateY[i].split(":")[1]-(-toTop ));\r\n                   Graphics.RestorePen();\r\n                }\r\n            }\r\n           for(var i=0 ; i < resprateY.length;  i++){\r\n                Graphics.SelectPen(1, GetColorValue(0, 0, 255), 0);\r\n                Graphics.Arc(  (resprateY[i].split(":")[0])-(-startX ), resprateY[i].split(":")[1]-(-toTop ), 3, 0, 360);\r\n                Graphics.RestorePen();\r\n            }\r\n       }\r\n\r\n\r\n\r\n//脉搏\r\nvar pulserateYs = Report.FieldByName("PULSERATEYS").AsString;\r\nif(pulserateYs !=""){\r\n\r\n           var pulserateY = pulserateYs.split(",");\r\n           for(var i = 0;i < pulserateY.length ; i++){\r\n             if(i>0){\r\n                 Graphics.SelectPen(1, GetColorValue(0, 0, 255), 0);\r\n                 Graphics.MoveTo((pulserateY[i-1].split(":")[0])-(-startX ),  pulserateY[i-1].split(":")[1]-(-340));\r\n                 Graphics.LineTo((pulserateY[i].split(":")[0])-(-startX ), pulserateY[i].split(":")[1]-(-340));\r\n                Graphics.RestorePen();\r\n             }\r\n          }\r\nfor(var i=0;i<pulserateY.length;i++){\r\n    Graphics.SelectTextColor(GetColorValue(0, 0, 255));\r\n    Graphics.DrawLabelText("●", (pulserateY[i].split(":")[0])-(-95.5), pulserateY[i].split(":")[1]-(-335)); \r\n    Graphics.RestoreTextColor();\r\n}\r\n    }\r\n\r\n//收缩压\r\nvar bpSysYs = Report.FieldByName("BPSYSYS").AsString;\r\nif(bpSysYs !=""){\r\n\r\n           var bpSysY = bpSysYs.split(",");\r\n           for(var i = 0;i < bpSysY.length ; i++){\r\n             if(i>0){\r\n                 Graphics.SelectPen(1, GetColorValue(0, 0, 255), 0);\r\n                 Graphics.MoveTo((bpSysY[i-1].split(":")[0])-(-startX ),  bpSysY[i-1].split(":")[1]-(-337));\r\n                 Graphics.LineTo( (bpSysY[i].split(":")[0])-(-startX ), bpSysY[i].split(":")[1]-(-337));\r\n                 Graphics.RestorePen();\r\n             }\r\n          }\r\nfor(var i=0;i<bpSysY.length;i++){\r\n    Graphics.SelectTextColor(GetColorValue(0, 0, 255));\r\n    Graphics.DrawLabelText("∨",  (bpSysY[i].split(":")[0])-(-96), bpSysY[i].split(":")[1]-(-330)); \r\n    Graphics.RestoreTextColor();\r\n}\r\n}\r\n\r\n\r\n//舒张压∧\r\nvar bpDiasYs = Report.FieldByName("BPDIASYS").AsString;\r\nif(bpDiasYs !=""){\r\n\r\n           var bpDiasY =bpDiasYs.split(",");\r\n           for(var i = 0;i < bpDiasY.length ; i++){\r\n             if(i>0){\r\n                 Graphics.SelectPen(1, GetColorValue(0, 0, 255), 0);\r\n                 Graphics.MoveTo((bpDiasY[i-1].split(":")[0])-(-startX ),  bpDiasY[i-1].split(":")[1]-(-327.5));\r\n                 Graphics.LineTo((bpDiasY[i].split(":")[0])-(-startX ), bpDiasY[i].split(":")[1]-(-327.5));\r\n                 Graphics.RestorePen();\r\n             }\r\n          }\r\nfor(var i=0;i<bpDiasY.length;i++){\r\n    Graphics.SelectTextColor(GetColorValue(0, 0, 255));\r\n    Graphics.DrawLabelText("∧",  (bpDiasY[i].split(":")[0])-(-96.5), bpDiasY[i].split(":")[1]-(-327)); \r\n    Graphics.RestoreTextColor();\r\n}\r\n}\r\n\r\n\r\n\r\n'
