        for(var i = 0;i < oPointlists.length; i++){\r\n              var ol = oPointlists [i] . split(":");\r\n               DrawHLines((ol[0])-(-100),338+(i*10),ol[1]);\r\n          }\r\n\r\n//入量\r\n//DrawTLines(156,293,300,293,"100ml");\r\n//DrawTLines(156,308,300,308,"100ml");\r\n\r\nvar iList = Report.FieldByName("inEvent").AsString;\r\n     var iPointlists = iList . split(",");\r\n          for(var i = 0;i < iPointlists.length; i++){\r\n              var il = iPointlists [i] . split(":");\r\n              // DrawHLines((ol[0])-(-100),338+(i*10),ol[1]);\r\n               DrawTLines( (il[0])-(-100) , 293+(i*15) , (il[1])-(-100) , 293+(i*15) , il[2] );\r\n          }\r\n\r\n\r\n//麻醉用药\r\nvar medList = Report.FieldByName("medEvent").AsString;\r\n var medPointlists = medList  . split(",");\r\n          for(var i = 0;i < medPointlists .length; i++){\r\n              var medl = medPointlists  [i] . split(":");\r\n             if(medl[0] =="0" ){\r\n                  DrawHLines((medl[1])-(-100),144+(i*10),medl[2]);\r\n                 }\r\n             if(medl[0] =="1" ){\r\n                  DrawTLines( (medl[1])-(-100) , 144+(i*15) , (medl[2])-(-100) , 144+(i*15) , medl[3] );\r\n                 }\r\n          }\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n'
