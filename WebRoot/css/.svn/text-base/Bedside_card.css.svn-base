@charset "utf-8";
/* CSS Document */


.container_left{ width:65%;margin-left:1%}
.container_left ul li{ width:200px; height:230px;float:left;}
.container_left ul li h1{ text-align:center; font-size:16px;}
.container_left ul li p{ width:200px; height:90px;}
.container_left ul li p a:focus{ background:#edecec;}
.boy{ background:#eff7ff; width:250px; border:1px solid #d6d6d6; margin-top:2%; margin-left:5.5%; border-radius:5px;}
.girl{ background:#fff6ff; width:250px; border:1px solid #d6d6d6; margin-top:2%; margin-left:5.5%;  border-radius:5px;}
.table{ margin:2%;}
td{ line-height:18px;}
input:focus{ background:#F5F5F5;}



.left_right{ width:33%;}
.left_right ul li{ border:1px solid #cacaca; width:232px; border-radius:5px; margin-top:4%; height:100%; float:left; margin-left:5%; overflow:hidden;} 
.left_right ul li h1{ text-align:center; font-size:18px; border-bottom:1px solid #cacaca; margin:0 2%; line-height:45px;}
.left_right ul li p{ border-bottom:1px solid #cacaca; margin:0 2%; line-height:45px;}
.button{ background:#FFFFFF; width:226px; border:none; line-height:45px;}
.last{ margin-bottom:2%;}
.ul{ height:100%;} 

.cx{ background:url(../images/31_03.png) no-repeat center center; width:68px; height:30px; border:none; color:#FFFFFF; }
.left_right ul li p a img{ vertical-align:middle; margin-top:4%; margin-left:1%;}
.left_right ul li p a:active{ background:#F5F5F5; display:block;}