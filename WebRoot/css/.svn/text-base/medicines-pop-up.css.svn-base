.theme-popover {
    z-index:9999;
    position:fixed;
    top:20%;
    left:10%;
    width:900px;
    height:400px;
    border:solid 5px  #1CAAD1;
    background-color: #f6f6f6;
    display:none;
    border-radius: 5px;
    font-size: 15px;
}
.head {
    position: absolute;
    height: 40px;
    width: 100%;
    border: 0px solid #1CAAD1;
    font-size: 30px;
    text-align: left;
    background-color: #1CAAD1;
    color: 	#FFFFFF;
    
}

.close {
    float:right;
    color:red;
    font-size: 40px;
}
.medicineList{
    position: absolute;
    width: 245px;
    height: 280px;
    border: 0px solid #aaaaaa;
    left: 1px;
    overflow :auto

}

.smdName{
	width: 190px;
	height: 20px;
	font-size:12px;
	text-align:left;
	border:solid 1px #FFFFFF;
	
}
.smdPinyin{
	width: 55px;
	height: 20px;
	font-size:12px;
	text-align:left;
	border:solid 1px #FFFFFF;
}


.medicines{
    position: absolute;
    float: left;
    width: 250px;
    height: 350px;
    border: 0px dotted dodgerblue;
    text-align: center;
    border-radius: 3px;
    margin-right: 5%;
    top: 45px;
    text-align:center;
     
}
.aMedicineList{
    position: absolute;
    left: 250px;
    width: 550px;
    height: 340px;
    border: 0px dotted dodgerblue;
    border-radius: 3px;
    font-size: 12px;
    top: 40px;
   
    /*margin-right: 5%;*/

}
a{
    text-decoration:none
}
.aMedicineList hr{
    color:red; 
    margin-left: 2%;
    margin-right: 2%;
    margin-top: 6px;
    margin-bottom: 6px;
    /*background-color: #ffffff;*/
}
.medicines hr{
	margin-left: 2%;
    margin-right: 2%;
    margin-top: 6px;
    margin-bottom: 6px;
}
.buttons{
    position: absolute;
    float: left;
    right: 15%;
    top:5%;
}
button{
    width: 85px;
    height: 35px;
    background-color: #1CAAD1;
    border-radius: 3px;
    border: 0px dotted  #000000;
}
button.close1{
    width: 85px;
    height: 75px;
    background-color: #1CAAD1;
    border-radius: 3px;
    border: 0px dotted  #000000;
}
button.close1:hover{
    background-color: #ff0000;
}
button:hover{
    background-color: darkgreen;
}
.buttons ul{
    position: absolute;
    top: 50%;
    left: 8%;
}
.buttons li{
    list-style:none;
    float:left;
    margin-top: 50px;

}
.fonts{
    margin-left: 5.5%;
}
.medicineName ,.medicalTakeWay ,.medicineDose,.medicineConcentration,.medicineSpeed{
    margin-left: 1%;
}
.medicineSpec,.medicineFirm{

}
th{
    background-color: #cecece;
    font-size: 13px;
}
.theme-popover-mask {
	z-index: 9998;
	position:fixed;
	top:0;
	left:0;
	width:100%;
	height:100%;
	background:#000;
	opacity:0.4;
	filter:alpha(opacity=40);
	display:none
}

