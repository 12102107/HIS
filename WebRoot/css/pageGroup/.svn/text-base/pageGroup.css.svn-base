/* CSS Document */
/*分页*/
#pageGro{ width:400px; height:25px; padding-top:30px; margin-bottom:2%;}
#pageGro div,#pageGro div ul li{ font-size:12px; color:#999; line-height:23px; float:left;}
#pageGro div ul li{ width:22px; text-align:center; border:1px solid #e0e8eb; cursor:pointer; background:#fafafa;}
#pageGro div ul li.on{ color:#fff; background:#6faed9; border:1px solid #3c90d9;}
#pageGro .pageUp,#pageGro .pageDown{ width:40px; border:1px solid #e0e8eb; cursor:pointer;}
#pageGro .pageUp{ text-indent:13px; background:url(../images/images/pageUp.png)5px 7px no-repeat;}
#pageGro .pageDown{ text-indent:15px; background:url(../images/images/pageDown.png)46px 6px no-repeat;}
